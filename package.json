{
  "name": "agro-conecta",
  "version": "1.0.0",
  "description": "PWA Marketplace que conecta produtores rurais a profissionais qualificados do agronegócio",
  "main": "index.js",
  "scripts": {
    "install:backend": "cd backend && npm install",
    "install:frontend": "cd frontend && pnpm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && pnpm run dev --host",
    "start:backend": "cd backend && npm start",
    "build:frontend": "cd frontend && pnpm run build",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && pnpm test",
    "deploy:build": "npm run build:frontend",
    "postinstall": "npm run install:all"
  },
  "keywords": [
    "agronegócio",
    "marketplace",
    "pwa",
    "react",
    "nodejs",
    "mongodb",
    "n8n"
  ],
  "author": "Agro-Conecta Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Moafsa/agroconecta.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}

